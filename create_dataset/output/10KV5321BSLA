<script type="text/javascript">
    $(function () {
        $(document).on("click", ".employerPopup", function () {
            showLoading();
            $.get(this.href, function (data) {
                $("#employer-modal-10KV5321BSLA").html(data);
                $("#employer-modal-container-10KV5321BSLA").modal("show");
            }).fail(function () {
                alert("There was a program retrieving the list of employers. Please try again.");
            }).always(function () {
                hideLoading();
            });
            return false;
        });

        $(document).on("click", ".gradSchoolPopup", function () {
            showLoading();
            $.get(this.href, function (data) {
                $("#gradschool-modal-10KV5321BSLA").html(data);
                $("#gradschool-modal-container-10KV5321BSLA").modal("show");
            }).fail(function () {
                alert("There was a program retrieving the list of grad schools. Please try again.");
            }).always(function () {
                hideLoading();
            });
            return false;
        });
    });
</script>
